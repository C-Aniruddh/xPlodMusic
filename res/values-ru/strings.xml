<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- App name -->
    <string name="app_name">Apollo</string>

    <!-- Content descriptions for the Bottom Action Bar -->
    <string name="cd_favorite">Сделать этот трек любимым</string>
    <string name="cd_search">Поиск по вашей музыке</string>
    <string name="cd_overflow">Больше возможностей</string>
    <string name="cd_bottom_action_bar_album_art">Обложка альбома для этого трека</string>
    <string name="cd_repeat">Повтор трека или плейлиста</string>
    <string name="cd_previous">Назад</string>
    <string name="cd_play">Воспр. или пауза</string>
    <string name="cd_next">Вперёд</string>
    <string name="cd_shuffle">Перемешать треки</string>

    <!-- AudioPlayer title -->
    <string name="nowplaying">Сейчас играет</string>

    <!-- Used to indicate the number of albums for an artist -->
    <plurals name="Nalbums">

        <!-- Number of albums is one -->
        <item quantity="one">1 альбом</item>
        <!-- Number of albums is more than one -->
        <item quantity="other"><xliff:g id="count">%d</xliff:g> альбома (ов)</item>
    </plurals>

    <!-- Used to indicate the number of songs for an album -->

    <plurals name="Nsongs">

        <!-- Number of songs is one -->
        <item quantity="one">1 трек</item>
        <item quantity="other"><xliff:g id="count">%d</xliff:g> трека (ов)</item>
    </plurals>

    <!-- Toasts after adding song(s) to playlists -->
    <plurals name="NNNtrackstoplaylist">

        <!-- message shown when one song was added -->
        <item quantity="one">1 трек добавлен в плейлист</item>
        <!-- message shown when zero or more than one song was added -->
        <item quantity="other"><xliff:g id="number" example="27">%d</xliff:g> трека (ов) добавлены в плейлист.</item>
    </plurals>

