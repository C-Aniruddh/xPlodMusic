    <!-- AudioPlayer title -->
    <string name="nowplaying">再生中</string>

    <!-- Used to indicate the number of albums for an artist -->
    <plurals name="Nalbums">

        <!-- Number of albums is one -->
        <item quantity="one">1つのアルバム</item>
        <!-- Number of albums is more than one -->
        <item quantity="other"><xliff:g id="count">%d</xliff:g>つのアルバム</item>
    </plurals>

    <!-- Used to indicate the number of songs for an album -->

    <plurals name="Nsongs">

        <!-- Number of songs is one -->
        <item quantity="one">1曲</item>
        <item quantity="other"><xliff:g id="count">%d</xliff:g>曲</item>
    </plurals>

    <!-- Toasts after adding song(s) to playlists -->
    <plurals name="NNNtrackstoplaylist">

        <!-- message shown when one song was added -->
        <item quantity="one">1つの曲をプレイイストに追加しました</item>
        <!-- message shown when zero or more than one song was added -->
        <item quantity="other"><xliff:g id="number" example="27">%d</xliff:g>つの曲をプレイイストに追加しました</item>
    </plurals>

