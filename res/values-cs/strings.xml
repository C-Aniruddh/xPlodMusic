    <!-- Used to indicate the number of albums for an artist -->
    <plurals name="Nalbums">

        <!-- Number of albums is one -->
        <item quantity="one">1 album</item>
        <!-- Number of albums is between 2-4 -->
        <item quantity="few"><xliff:g id="count">%d</xliff:g> alba</item>
        <!-- Number of albums is more than 4 -->
        <item quantity="other"><xliff:g id="count">%d</xliff:g> alb</item>
    </plurals>

    <!-- Used to indicate the number of songs for an album -->

    <plurals name="Nsongs">

        <!-- Number of songs is one -->
        <item quantity="one">1 skladba</item>
        <item quantity="few"><xliff:g id="count">%d</xliff:g> skladby</item>
        <item quantity="other"><xliff:g id="count">%d</xliff:g> skladeb</item>
    </plurals>

    <!-- Toasts after adding song(s) to playlists -->
    <plurals name="NNNtrackstoplaylist">
        <!-- message shown when one song was added -->
        <item quantity="one">Jedna skladba přidána do playlistu</item>
        <!-- message shown when zero or more than one song was added -->
        <item quantity="few"><xliff:g id="number" example="27">%d</xliff:g> skladby přidány do playlistu</item>
        <item quantity="other"><xliff:g id="number" example="27">%d</xliff:g> skladeb přidáno do playlistu</item>
    </plurals>

